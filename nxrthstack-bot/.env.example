# Discord Bot Configuration
DISCORD_BOT_TOKEN=           # Bot token from Discord Developer Portal
DISCORD_CLIENT_ID=           # Application ID (e.g., 1468315439309262981)
DISCORD_CLIENT_SECRET=       # Shared secret for API auth (same value in Vercel)
DISCORD_GUILD_ID=            # Your server ID (for dev command sync)

# Database (same as web app - get from Neon dashboard)
DATABASE_URL=                # PostgreSQL connection string

# NxrthStack Website URL (no /api at the end)
NXRTH_API_URL=https://nxrthstack.vercel.app

# Bot API Server (for receiving webhooks from web app)
API_PORT=3001
